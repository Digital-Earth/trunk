@keyframes bounce {
	0%, 20%, 50%, 80%, 100% {
		transform-property(translateY(0))
	}
	40% {
		transform-property(translateY(-32px))
	}
	60% {
		transform-property(translateY(-16px))
	}
}

.bounce
	vendor('animation', bounce 1.3s 2)

