.hamburger 
	cursor: pointer
	display: inline-block
	margin-left: px(16)
	margin-top:px(-($hamburger-top / 2))
	padding: px($hamburger-top) 0
	position: relative 
	user-select: none
	vertical-align: middle
	transition(0.15s)

.hamburger__icon:before
.hamburger__icon:after 
	transition(0.15s)

.link--open:hover .hamburger 
	transition(0.3s)

.link--open:hover .hamburger__icon:before
.link--open:hover .hamburger__icon:after 
	transition(all 0.15s)

.link--open:hover .hamburger__icon:before 
	top: px($hamburger-top + ($hamburger-space / 2))

.link--open:hover .hamburger__icon:after 
	top: px(($hamburger-top + ($hamburger-space / 2)) * -1)

.hamburger__icon 
	background: site-color['white']
	height: px($hamburger-height)
	position: relative
	width: px($hamburger-width)
	transition(0.1s)

.hamburger__icon:before
.hamburger__icon:after 
	display: block
	content: ''
	height: px($hamburger-height)
	width: px($hamburger-width)
	background: site-color['white']
	position: absolute
	z-index: -1
	transition(0.2s 0.1s)

.hamburger__icon:before 
	top: px($hamburger-top)

.hamburger__icon:after 
	top: px($hamburger-top * -1)

.hamburger.is-active:not(.steps) .hamburger__icon:before
.hamburger.is-active:not(.steps) .hamburger__icon:after 
	top: 0 !important

.hamburger.is-active .hamburger__icon:before
.hamburger.is-active .hamburger__icon:after 
	transition(0.4s)

.hamburger.is-active .hamburger__icon 
	background: transparent

.hamburger.is-active .hamburger__icon:before 
	transform: rotate(-45deg)

.hamburger.is-active .hamburger__icon:after 
	transform: rotate(45deg)

