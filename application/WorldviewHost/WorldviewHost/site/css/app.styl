.link 
	color: inherit
	text-decoration: none

.link--medium
	font-size: font-sizes['medium']
	
.link--large 
	font-size: font-sizes['large']
	
.link--normal
	color: font-color['dark-secondary'] 
	text-decoration: none
	background-image: -webkit-linear-gradient(font-color['light-primary'] 0%, font-color['dark-secondary'] 100%)
	background-image: linear-gradient(font-color['light-primary'] 0%, font-color['dark-secondary'] 100%)
	background-repeat: repeat-x
	background-size: 1px 1px
	background-position: 0 95%
	text-shadow: 3px 0 site-color['white'], 2px 0 site-color['white'], 1px 0 site-color['white'], -1px 0 site-color['white'], -2px 0 site-color['white'], -3px 0 site-color['white']
 	
@media (-webkit-min-device-pixel-ratio: 1.75), (min-resolution: 168dpi)
	.link--normal 
		background-image: -webkit-linear-gradient(font-color['light-primary'] 0%, font-color['dark-secondary'] 100%)
		background-image: linear-gradient(font-color['light-primary'] 0%, font-color['dark-secondary'] 100%)
		background-position: 0 93%;

.link--normal:hover 
	color: site-color['pop-primary']
	background-image: -webkit-linear-gradient(top, site-color['pop-primary'] 0%, site-color['pop-primary'] 100%)
	background-image: linear-gradient(to bottom, site-color['pop-primary'] 0%, site-color['pop-primary'] 100%)

.link--normal ::-moz-selection
.link--normal  > *::-moz-selection 
	background-color: #c9dbb0
	color: #57534a
	text-shadow: none

.link--normal::selection
.link--normal > *::selection 
	background-color: #c9dbb0
	color: #57534a
	text-shadow: none

.link--normal::-moz-selection
.link--normal > *::-moz-selection 
	background-color: #c9dbb0
	color: #57534a
	text-shadow: none

h1 .link--normal
h2 .link--normal
h3 .link--normal 
	background-size: 1px 2px
	
@media (-webkit-min-device-pixel-ratio: 1.75), (min-resolution: 168dpi) 
	h1 .link--normal
	h2 .link--normal
	h3 .link--normal 
		background-position: 0 93%
		background-image: -webkit-linear-gradient(font-color['dark-secondary']  0%, font-color['dark-secondary']  100%)
		background-image: linear-gradient(font-color['dark-secondary']  0%, font-color['dark-secondary']  100%)
		background-size: 1px 1px
		background-position: 0 93%
		
.link--block 
	background-color: transparent 

.link--block:hover 
	background-color: site-color['white']
	color: site-color['black']

.link--underline
	color: site-color['black']
	position: relative
	
.link--underline:before 
	background: site-color['black']
	bottom: -3px
	content: ''
	height: 1px
	left: 0
	position: absolute
	visibility: hidden
	width: 100%
	transform-property(scaleX(0))
	transition(all 0.4s ease-in-out-expo 0s)

.link--underline:hover:before 
	visibility: visible
	transform-property(scaleX(1))

.link--underline.reverse:before
	transform-property(scaleX(1)) 
	visibility: visible

.link--underline.reverse:hover:before
	transform-property(scaleX(0)) 
	visibility: hidden

.link--swap img:last-child 
	display: none

.link--swap:hover img:first-child
	display: none

.link--swap:hover img:last-child
