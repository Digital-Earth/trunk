    window.globeConfig = {
        disableZoomControls: false,
        defaultTheme: 'landingGlobeConfig',
        enableGlobeSpin: true, // wait until load to set this
        rangeLimitMin: 500000.0,  // in meters
        HoverDisplay: {
          "6c733647-8e86-454f-a4d9-6a8ed153347d": {
