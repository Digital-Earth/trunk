<body ng-keyup="globeKeyUp($event)" ng-cloak>

<!-- == Landing Header == -->
<header ng-include="'/site/parts/global-header.html'" class="global-header"></header>

<!-- == Main Call To Action == -->
<div id="landing-container" class="landing__banner text-center disablemouse" fade-in-intro fade-in-init="demoInfo.set('overlayIsActive', true);" fade-in-done="demoInfo.set('exploreIsActive', true);">
    <div class="header-inner">
        <h1 class="headline-landing display-light">
            <span class="headline-landing__brandname">PYXIS WorldView.</span> 
            <span class="headline-landing__tagline" ng-if="supports.webgl">Where your globe comes to life.</span>
            <span class="headline-landing__tagline text-small" ng-if="!supports.webgl">It looks like your browser doesn't support WebGl. Don't worry you can still explore the WorldView Gallery.</span>
        </h1>
        <div class="headline-landing__description display-light">
            <a class="navigation__item reverse go-next" href="" ng-click="skipToEnd()"><span>Next</span> <span class="icon nc-icon-glyph arrows-1_tail-right"></span></a>
            <p>On any given day we have questions about our world. We expect answers.</p>
            <p>The globe is your canvas. Search and explore the Earth. Find places and discover spaces.</p>
            <p>This is PYXIS WorldView, a new platform designed for business, education and human curiosity.</p>
        </div>

        <a id="demo-link" class="btn btn--small btn--rounded btn--white get-exploring " href="" ng-if="supports.webgl">Explore now</a>
        <a class="btn btn--small btn--rounded btn--white" href="/browse" target="_self" ng-if="!supports.webgl">Explore the gallery</a>
        <a class="btn btn--small btn--rounded btn--white" href="" ng-if="userDevice === 'Mobile' && supports.webgl" ng-click="captureEmail.toggle($event);">Get early access</a>
    </div>
</div>

<!-- == Demo Legend  == -->
<div ng-if="demoInfo.showLegend" demo-legend legend-map="currentMap" legend-prune="['8be6a2ec-5110-49cf-a295-1008f8e9a21b', '7664e383-9d2c-494d-b3ec-5ae708623565']"  legend-event="{'on': 'iconHoverStart', 'off': 'iconHoverEnd'}" ng-include="'/site/parts/static-legend.html'"></div>

<!-- == Templates  == -->
<div ng-repeat="url in templates.sections" ng-include="url"></div>
<div ng-repeat="url in templates.dialogs" ng-include="url"></div>
<div ng-repeat="url in templates.widgets" ng-include="url"></div>

<div id="Intercom" ng-show="intercomEnabled" class="landing">
</div>

<div class="loading-bar"><span></span></div>

<!-- == Landing Footer == -->
<footer ng-include="'/site/parts/global-footer.html'" class="global-footer"></footer>

<!-- == Modals == -->
    <div class="modal demo-info-modal slide-up-bottom" ng-class="{'is-visible is-open': demoInfo.exploreIsActive}" ng-include="'/site/parts/modals/demo-info.html'" ensure-even-dimensions></div>
    <div class="modal share-modal slide-up-bottom" ng-class="{'is-visible': demoInfo.shareIsActive}"  ng-include="'/site/parts/modals/share.html'"></div>
    <div class="static-modal-overlay" ng-if="demoInfo.shareIsActive || demoInfo.exploreIsActive || demoInfo.overlayIsActive" transition-props="{autoAlpha: 1}" transition-duration="0.4"></div>

<!-- == Globe Tour == -->
<div class="tour-bubble" feature-tour="globeTour">
    <div class="tour-bubble__wrapper" ng-if="tourCommands.play">
        <div class="tour-bubble__container" ng-if="tourCommands.partSection">
            <div class="tour-bubble__arrow" ng-if="tourCommands.partSection.arrowUrl">
                <img ng-src="{{tourCommands.partSection.arrowUrl}}" />
            </div>
            <div class="tour-bubble__header">
                <span>{{tourCommands.partSection.headline}}</span>
            </div>
            <p class="tour-bubble__caption">
                {{tourCommands.insert('tour-bubble__caption', tourCommands.partSection.caption)}}
            </p>
            <div class="tour-bubble__footer">
                <a class="tour-next navigation__item reverse" href="" ng-if="tourCommands.getIndex() < tourCommands.currentPart.steps.length - 1" ng-click="tourCommands.control('next')">Next</a>
                <a class="tour-next navigation__item reverse" href="" ng-if="tourCommands.getIndex() === tourCommands.currentPart.steps.length - 1" ng-click="tourCommands.kill()">Done</a>
                <a class="tour-prev navigation__item reverse" href="" ng-if="tourCommands.getIndex() > 0" ng-click="tourCommands.control('prev')">Prev</a>
            </div>
        </div>
    </div>

<script>
// used to turn on theme controls
window.themeControlsEnabled = false;

function animateCamera() {
    window.GC.config.enableGlobeSpin = true;

    // fade in the globe
    TweenMax.to('#globe-default', 10.0, {opacity: 1.0, ease: Quad.easeOut});
}

TweenMax.set('#globe-default', {autoAlpha: 0});
setTimeout(animateCamera, 5500);

</script>

</body>
</html>